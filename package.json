{
  "name": "learn-to-code",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server.js",
    "client": "cd client && npm run dev",
    "build": "cd client && npm run build",
    "start": "node server.js",
    "lint": "eslint src --ext .js,.jsx",
    "format": "prettier --write src/**/*.{js,jsx,css,html}"
  },
  "dependencies": {
    "axios": "^1.6.2",
    "bcryptjs": "^2.4.3",
    "classnames": "^2.3.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.0",
    "express": "^4.19.2",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.3.1",
    "nodemailer": "^6.9.9",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "react-router-dom": "^6.16.0",
    "jwt-decode": "^3.1.2",
    "concurrently": "^8.2.0"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "latest",
    "autoprefixer": "latest",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "latest",
    "eslint-plugin-react": "latest",
    "postcss": "latest",
    "prettier": "latest",
    "tailwindcss": "latest",
    "vite": "latest",
    "nodemon": "latest"
  }
}